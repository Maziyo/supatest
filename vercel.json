{
    "version": 2,
    "builds": [
      {
        "src": "api/test.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/test",
        "dest": "/api/test.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  